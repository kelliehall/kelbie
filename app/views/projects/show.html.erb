<div class="skinnyWrapper wrapperPadding" id='post_show_content'>
      <header>
        <p class="date"><%= @project.created_at.strftime('%A, %b %d') %></p>
        <h1><%= @project.title %></h1>
        <hr>
      </header>
      <div id="content">
        <%= markdown @project.description %>
        <div class="buttonWrapper">
          <a href="<%= @project.link %>" class='projectButton' target= '_blank'>Visit <%= @project.title %></a>
        </div> 
      </div>
      <% if user_signed_in? %>
        <div id="adminLinks">
          <%= link_to 'Edit', edit_project_path(@project) %>
          <%= link_to 'Delete', project_path(@project),method: :delete, data: {confirm:'Are you sure?'} %>
        </div>
    <% end %>
      <hr>
</div>
